import re

txt = "The rain in Spain"
x = re.findall("ai", txt)
print(x)


#Search for an upper case "S" character in the beginning of a word, and print the word:

txt = "The rain in Spain"
x = re.search(r"\bS\w+", txt)
print(x.group())
